<!--miniprogram/pages/page_admin_book_mgmt/page_admin_book_mgmt.wxml-->
<van-nav-bar title="教材预订管理" left-text="返回" right-text="注销" left-arrow bind:click-left="onClickLeft" bind:click-right="onClickRight" />
<view>
  <van-collapse value="{{ activeNames }}" bind:change="onChangeCollapse">
    <van-collapse-item title="筛选" name="1">
      筛选选项
    </van-collapse-item>
  </van-collapse>
  <view class="container">
    <van-button type="primary" bind:click="btn_search">点击查询</van-button>
  </view>
  <view id="showContain" style='width=80%'>
    <view wx:for="{{order_list}}">
      <van-panel title="{{item.order_semester}} - {{item.order_course}}" desc="《{{item.order_book_name}}》" status="{{item.order_college}} {{item.order_grade}}级 - {{item.order_major}}">
      </van-panel>
      <br/>
    </view>
  </view>
</view>