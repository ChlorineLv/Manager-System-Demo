<!--miniprogram/pages/page_admin_book/page_admin_book.wxml-->
<text>教材预定</text>
<van-cell-group>
  <van-field value="{{ username }}" required clearable label="用户名" icon="question-o" placeholder="请输入用户名" bind:click-icon="onClickIcon" />
  <van-field id="order_college" required clearable label="学院/系别名称" placeholder="请输入学院/系别名称" />
  <van-field id="order_major" required clearable label="专业名称" />
  <van-field id="order_grade" required clearable label="年级" />
  <van-field id="order_semester" required clearable label="学期"/>
  <van-field id="order_course" required clearable label="课程名称"/>
  <van-field id="order_teacher" clearable label="任课老师名字"/>
  <van-field id="order_book_name" required clearable label="教材名称"/>
  <van-field id="order_book_isbn" required clearable label="教材ISBN"/>
  <van-field id="order_book_writer" clearable label="作者/编著者"/>
  <van-field id="order_book_writer" clearable label="版次" />
  <van-field id="order_book_publisher" clearable label="出版社" />
  <van-field id="order_book_price" required clearable label="单价"/>
  <van-field id="order_remark" clearable label="备注" placeholder="选填" />

</van-cell-group>